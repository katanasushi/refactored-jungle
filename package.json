{
  "name": "cypress-boiler-plate",
  "version": "1.0.0",
  "description": "implementation of cypress framework for testing links",
  "main": "index.js",
  "scripts": {
    "cy:run": "sh cy-runner.sh",
    "test:chrome": "cypress run --browser chrome --spec cypress/integration/mytests/dead.spec.js,cypress/integration/examples/aliasing.spec.js",
    "combine:report": "mochawesome-merge ./mochawesome-report/*.json > index.json",
    "generate:report": "marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^9.6.0"
  },
  "devDependencies": {
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  }
}
